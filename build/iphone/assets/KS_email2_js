function CheckEmailaddress(){current_email=aTextField.value}function emailCurrentText(){emailButton.removeEventListener("click",emailCurrentText),emailButton.addEventListener("click",emailCurrentText);var e=Titanium.UI.createEmailDialog();e.subject="SiWriter "+new Date,e.toRecipients=[aTextField.value],e.messageBody="Message written: "+new Date+"\n\n\n"+txtViewDesc.value,e.setHtml(!1),e.setBarColor("#336699"),e.removeEventListener("complete",function(){}),e.addEventListener("complete",function(t){alert(t.result==e.SENT?"message was sent":"message was not sent.")}),e.open()}