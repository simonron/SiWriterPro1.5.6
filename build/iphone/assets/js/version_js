function isIPhone3_2_Plus(){if("iPhone OS"==Titanium.Platform.name){var e=Titanium.Platform.version.split("."),t=parseInt(e[0],10),i=parseInt(e[1],10);if(t>3||3==t&&i>1)return!0}return!1}function isiOS4Plus(){if("iPhone OS"==Titanium.Platform.name){var e=Titanium.Platform.version.split("."),t=parseInt(e[0],10);if(t>=4)return!0}return!1}function isiOS6Plus(){if("iPhone OS"==Titanium.Platform.name){var e=Titanium.Platform.version.split("."),t=parseInt(e[0],10);if(t>=6)return!0}return!1}function isiOS7Plus(){if("iPhone OS"==Titanium.Platform.name){var e=Titanium.Platform.version.split("."),t=parseInt(e[0],10);if(t>=7)return!0}return!1}