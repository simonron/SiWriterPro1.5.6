"use strict";var utterance=require("bencoding.utterance"),speech=utterance.createSpeech();speech.addEventListener("started",function(e){Ti.API.info(JSON.stringify(e))}),speech.addEventListener("completed",function(e){Ti.API.info(JSON.stringify(e))}),speech.addEventListener("paused",function(e){Ti.API.info(JSON.stringify(e))}),speech.addEventListener("canceled",function(e){Ti.API.info(JSON.stringify(e))}),speech.addEventListener("continued",function(e){Ti.API.info(JSON.stringify(e))});var pauseButton=Ti.UI.createButton({bottom:10,title:"Pause",left:60,width:50,height:50});pauseButton.addEventListener("click",function(){return speech.isSpeaking?void speech.pauseSpeaking():void Ti.API.info("Nothing to pause, press play first")});var continueButton=Ti.UI.createButton({bottom:10,title:"Continue",left:110,width:80,height:50});continueButton.addEventListener("click",function(){return speech.isSpeaking?void Ti.API.info("Already speaking, nothing to continue"):void speech.continueSpeaking()});var stopButton=Ti.UI.createButton({bottom:10,title:"Stop",left:190,width:50,height:50});stopButton.addEventListener("click",function(){return speech.isSpeaking?void speech.stopSpeaking():void Ti.API.info("nothing to stop, press play first")}),win1.addEventListener("open",function(){utterance.isSupported()||alert("sorry you need iOS7 or greater")});